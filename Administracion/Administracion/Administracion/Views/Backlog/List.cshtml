@model IEnumerable<Administracion.Models.TicketViewModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="panel-heading">            
    <h3><span class="label label-primary"> Tickets</span></h3>
</div>


@if (Model.Count() > 0)
{
    <div class="panel-body">
        @foreach(var ticket in Model)
        {
            @Html.Partial("_TicketItem", ticket)
        }
        
    </div>

    
}
else
{
    @Html.Partial("_EmptyBacklog")
}